{
  "name": "jira-project-page-custom-ui",
  "version": "1.1.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "packageManager": "bun@1.2.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "./clean.sh",
    "deploy:forge:dev": "cd apps/forge-app && forge deploy",
    "deploy:forge:prod": "cd apps/forge-app && forge deploy -e production",
    "build:web": "turbo run build --filter=web",
    "build:manifest": "turbo run build --filter=manifest",
    "build:utils": "turbo run build --filter=utils",
    "build:remote": "turbo run build --filter=remote-app",
    "deploy:remote": "turbo run deploy --filter=remote-app",
    "full:remote": "turbo run build --filter=remote-app && turbo run deploy --filter=remote-app"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "turbo": "^2.4.4"
  },
  "dependencies": {
    "chart.js": "^4.4.8",
    "react-chartjs-2": "^5.3.0",
    "recharts": "^2.15.1"
  }
}